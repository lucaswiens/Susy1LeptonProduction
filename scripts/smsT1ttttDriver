#!/bin/sh
#Step 1
cmsDriver.py Configuration/GenProduction/python/SUS-RunIIAutumn18FSPremix-00006-fragment.py --python_filename python/SUS-T1tttt_1_PREMIXRAW_cfg.py --eventcontent PREMIXRAW --customise Configuration/DataProcessing/Utils.addMonitoring --datatier GEN-SIM-RAW --fileout file:SUS-T1tttt_1_PREMIXRAW.root --conditions 102X_upgrade2018_realistic_v15 --beamspot Realistic25ns13TeVEarly2018Collision --customise_commands "process.source.numberEventsInLuminosityBlock = cms.untracked.uint32(200)"\\nprocess.source.numberEventsInLuminosityBlock="cms.untracked.uint32(400)" --pileup_input das:/Neutrino_E-10_gun/RunIIFall17FSPrePremix-PUMoriond18_102X_upgrade2018_realistic_v15-v2/GEN-SIM-DIGI-RAW --datamix PreMix --procModifiers premix_stage2 --step GEN,SIM,DIGI,DATAMIX,L1,DIGI2RAW,HLT:@relval2018 --era Run2_2018 --mc --no_exec
#Step 2
cmsDriver.py  --python_filename python/SUS-T1tttt_2_AODSIM_cfg.py --eventcontent AODSIM --customise Configuration/DataProcessing/Utils.addMonitoring --datatier AODSIM --fileout file:SUS-T1tttt_2_AODSIM.root --conditions auto:phase1_2018_realistic --procModifiers premix_stage2 --step RAW2DIGI,L1Reco,RECO,RECOSIM,EI --filein file:SUS-T1tttt_1_PREMIXRAW.root --era Run2_2018 --runUnscheduled --mc --no_exec
#Step 3
cmsDriver.py  --python_filename python/SUS-T1tttt_3_MiniAOD_cfg.py --eventcontent MINIAODSIM --customise Configuration/DataProcessing/Utils.addMonitoring --datatier MINIAODSIM --runUnscheduled --procModifiers premix_stage2 --fileout file:SUS-T1tttt_3_MiniAOD.root --conditions auto:phase1_2018_realistic --step PAT --filein file:SUS-T1tttt_2_AODSIM.root --era Run2_2018 --mc --no_exec

# BACKUP
